<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_today">

    <fragment
        android:id="@+id/navigation_today"
        android:name="com.mdemel.sendemoapp.ui.today.TodayFragment"
        android:label="Today"
        tools:layout="@layout/fragment_today" />

    <fragment
        android:id="@+id/navigation_news"
        android:name="com.mdemel.sendemoapp.ui.news.NewsFragment"
        android:label="News"
        tools:layout="@layout/fragment_news" />

    <fragment
        android:id="@+id/navigation_scheduele"
        android:name="com.mdemel.sendemoapp.ui.scheduele.SchedueleFragment"
        android:label="Scheduele"
        tools:layout="@layout/fragment_scheduele" />

    <fragment
        android:id="@+id/navigation_players"
        android:name="com.mdemel.sendemoapp.ui.players.PlayersFragment"
        android:label="Players"
        tools:layout="@layout/fragment_players"/>

    <fragment
        android:id="@+id/navigation_tickets"
        android:name="com.mdemel.sendemoapp.ui.tickets.TicketsFragment"
        android:label="Tickets"
        tools:layout="@layout/fragment_tickets"/>
</navigation>